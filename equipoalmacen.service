[Unit]
Description=Equipo Almacen de Logistica

[Service]
ExecStart=node /home/cyriljosepha/back_logistica/app.js
Restart=always
User=cyriljosepha
# Note Debian/Ubuntu uses 'nogroup', RHEL/Fedora uses 'nobody'
Group=nogroup
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/home/cyriljosepha/back_logistica

[Install]
WantedBy=multi-user.target
